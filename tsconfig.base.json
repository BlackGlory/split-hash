{
  "compilerOptions": {
    "target": "ES2019"
  , "module": "CommonJS"
  , "strict": true
  , "noUnusedLocals": false
  , "noUnusedParameters": false
  , "baseUrl": "."
  , "paths": {
      "@nodejs": ["src/nodejs/index"]
    , "@nodejs/*": ["src/nodejs/*"]
    , "@whatwg": ["src/whatwg/index"]
    , "@whatwg/*": ["src/whatwg/*"]
    , "@test/*": ["__tests__/*"]
    }
  }
}
